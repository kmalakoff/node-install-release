{
  "name": "node-install-release",
  "version": "1.3.0",
  "description": "Cross-platform solution for installing releases of Node.js",
  "keywords": [
    "node",
    "version",
    "versions",
    "install",
    "dist",
    "release",
    "releases"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/node-install-release.git"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "bin": {
    "nir": "./bin/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "core-js": "^3.39.0",
    "cross-spawn-cb": "^1.0.0",
    "end-with": "^1.0.2",
    "exit": "^0.1.2",
    "fast-extract": "^1.2.1",
    "fs-access-compat": "^1.0.3",
    "function-exec-sync": "^0.2.11",
    "get-remote": "^1.0.0",
    "getopts-compat": "^2.2.5",
    "isarray": "^2.0.5",
    "lodash.find": "^4.6.0",
    "lodash.keys": "^4.2.0",
    "mkpath": "^1.0.0",
    "node-filename-to-dist-paths": "^0.2.5",
    "node-resolve-versions": "^1.0.0",
    "osenv": "^0.1.5",
    "pump": "^3.0.2",
    "queue-cb": "^1.2.1",
    "rimraf2": "^2.7.1",
    "single-line-log2": "^1.1.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.1",
    "cr": "^0.1.0",
    "depcheck": "^1.4.7",
    "fs-access-sync-compat": "^1.0.2",
    "is-version": "^0.2.1",
    "node-version-utils": "^0.5.6",
    "ts-dev-stack": "^0.16.2"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs"
    ]
  }
}
