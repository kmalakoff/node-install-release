{
  "name": "node-install-release",
  "version": "1.13.11",
  "description": "Cross-platform solution for installing releases of Node.js",
  "keywords": [
    "node",
    "version",
    "versions",
    "install",
    "dist",
    "release",
    "releases"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/node-install-release.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.cts",
  "bin": {
    "nir": "bin/cli.js"
  },
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "biome check --write --unsafe",
    "test": "mocha --no-timeouts test/**/*.test.*",
    "test:engines": "nvu 0.8,0.10,0.12,4,12,16,lts,stable tsds test:node --no-timeouts",
    "version": "tsds version"
  },
  "dependencies": {
    "cross-spawn-cb": "^2.3.10",
    "exit": "^0.1.2",
    "fast-extract": "^1.6.4",
    "function-exec-sync": "^1.4.4",
    "get-remote": "^2.0.10",
    "getopts-compat": "^2.2.6",
    "homedir-polyfill": "^1.0.3",
    "is-version": "^1.0.1",
    "lodash.keys": "^4.2.0",
    "mkdirp-classic": "^0.5.3",
    "node-filename-to-dist-paths": "^1.2.10",
    "node-resolve-versions": "^1.2.8",
    "on-one": "^1.0.1",
    "pump": "^3.0.3",
    "queue-cb": "^1.5.5",
    "rimraf2": "^2.8.2"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*",
    "cr": "*",
    "lodash.find": "*",
    "lodash.values": "*",
    "node-version-compare": "*",
    "node-version-utils": "*",
    "pinkie-promise": "*"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}
