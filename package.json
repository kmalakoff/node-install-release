    "fast-extract": "^0.4.8",
    "fs-access-compat": "^1.0.3",
    "function-exec-sync": "^0.2.11",
    "get-remote": "^0.8.3",
    "getopts-compat": "^2.2.5",
    "isarray": "^2.0.5",
    "lodash.find": "^4.6.0",
    "lodash.keys": "^4.2.0",
    "mkpath": "^1.0.0",
    "node-filename-to-dist-paths": "^0.2.4",
    "node-resolve-versions": "^0.3.9",
    "osenv": "^0.1.5",
